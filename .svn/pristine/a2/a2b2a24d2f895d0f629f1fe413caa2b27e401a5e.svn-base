<script type="text/html" id="mulu_tpl">
	{{each list as row i}}
		<li>
			<a href="{{row.link}}" class="ellipsis" title="">{{row.title}}</a>
			{{if row.isvip ==1}}
				{{if row.isorder == 1}}
		            <span class="moneyor unlock"></span>
				{{else}}
					<span class="moneyor lock"></span>
				{{/if}}
			{{else}}
                <span class="moneyor">免费</span>
			{{/if}}
		</li>
	{{/each}}
</script>