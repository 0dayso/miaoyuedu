<script type="text/html" id="updown_tpl1">
    {{if prechapter.chpid == ''}}
	    <li><a href="javascript:void(0);" class="disable">没有了</a>
	{{else}}
		<li><a href="{{prechapter.uplink}}" id="prechp">上一章</a>
	{{/if}}
		<span class="rdcon_mulu_jishi">键盘快捷键“←”</span></li>
	    <li><a href="{{mulu}}" id="rd_mulu"><span  class="rd_mulu"><b class="ic_mulu">{{chpnum}}/{{totalchapnum}}</b></span><span class="rdcon_mulu_jishi">键盘快捷键“Enter”</span></a></li>
	{{if nextchapter.chpid == ''}}
	    <li><a href="javascript:void(0);" class="disable">没有了</a>
	{{else}}
		<li><a href="{{nextchapter.downlink}}" id="nextchp">下一章</a>
    {{/if}}
	    <span class="rdcon_mulu_jishi">键盘快捷键“→”</span></li>
</script>
<script type="text/html" id="updown_tpl2">
    {{if prechapter.chpid == ''}}
	    <a href="javascript:void(0);" class="rdset_up disable"><span class="ic_set_up"></span>没有了</a>
    {{else}}
	    <a href="{{prechapter.uplink}}" class="rdset_up"><span class="ic_set_up"></span>上一章</a>
    {{/if}}
	{{if nextchapter.chpid == ''}}
         <a href="javascript:void(0);" class="rdset_down disable"><span class="ic_set_down"></span>没有了</a>
    {{else}}
	    <a href="{{nextchapter.downlink}}" class="rdset_down"><span class="ic_set_down"></span>下一章</a>
    {{/if}}
</script>